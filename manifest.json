{
  "name": "划词翻译",
  "description": "",
  "manifest_version": 2,
  "version": "1.0.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
  "incognito": "split",
  "icons": {
    "128": "assets/dictionary.svg",
    "16": "assets/dictionary.svg",
    "48": "assets/dictionary.svg"
  },
  "author": "",
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "storage",
    "clipboardWrite",
    "clipboardRead",
    "activeTab",
    "identity",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "ctx/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*"
      ],
      "js": [
        "ctx/content.js"
      ],
      "css": [
        "ctx/content.css"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "ctx/background.js",
    "assets/*"
  ]
}